{
  "manifest_version": 2,
	"name": "WebCT MultiTab",
	"version": "1.0.5",
	"description": "Extension which allows multiple WebCT tabs to be opened simultaneously.",
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
  "permissions": [ "cookies", "*://vista.unm.edu/" ],
  "background": {
    "scripts": [ "background.js" ]
  },
	"options_page": "options.html"
}